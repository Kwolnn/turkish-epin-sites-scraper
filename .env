# Server Configuration
PORT=3000

# N8N Webhook Configuration (DISABLED - using direct HTTP request instead)
N8N_WEBHOOK_URL=http://n8n:5678/webhook/game-data
ENABLE_N8N_WEBHOOK=false
N8N_BASIC_AUTH_USER=your_email@example.com
N8N_BASIC_AUTH_PASSWORD=your_password
N8N_RESUME_URL=https://your-ngrok-url.ngrok-free.app/webhook-waiting/174

# Scraping Configuration
SCRAPER_CONCURRENCY=2
SCRAPER_BATCH_DELAY=10000
SCRAPER_REQUEST_DELAY=3000
SCRAPER_DOMAIN_DELAY=0
MAX_EXECUTION_TIME=3600000

# FlareSolverr Configuration
FLARESOLVERR_URL=http://flaresolverr:8191

# User Agent
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=gameprices
DB_USER=postgres
DB_PASSWORD=your_db_password
DB_SSL=false

# Optional: Connection pool settings
DB_MAX_CONNECTIONS=20
DB_IDLE_TIMEOUT=30000
DB_CONNECTION_TIMEOUT=2000